#include <iostream>
using namespace std;
void is_prime()
{
    int x;
    cout << "enter the value of x = " << endl;
    cin >> x;
    if(x/1==x && x/x==1)
    {
        cout << "it is a prime number" << endl;
    }
    else
    {
        cout << "it is not a prime number" << endl;
    }
}
int main()
{
    is_prime();
    return 0;
}
