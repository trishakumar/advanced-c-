#include <iostream>

using namespace std;

int main()
{
    int x, y;
    cout << "enter 2 numbers = " << endl;
    cin >> x >> y;
    cout << "first number = " << x << endl;
    cout << "second number = " << y << endl;
    cout << "after swapping ...." << endl;
    cout << "first number = " << y << endl;
    cout << "second number = " << x << endl;
    return 0;
}
